# .coveragerc
[run]
omit =
    */migrations/*
    */tests/*
    */admin.py
    */apps.py
    manage.py
    config/*
branch = True

[report]
show_missing = True
skip_covered = True
exclude_lines =
    pragma: no cover
    def __str__
    if self\.debug
    if settings\.DEBUG

[html]
directory = htmlcov